<Window x:Class="Sport_Application_Teacher__New_Design_.Pages.ReportStudHoursWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sport_Application_Teacher__New_Design_.Pages"
        mc:Ignorable="d"
        Title="ReportStudHoursWindow" Height="600" Width="1080" WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <DataGrid x:Name="studHoursGrid" FontSize="22" AutoGeneratingColumn="studHoursGrid_AutoGeneratingColumn"/>
        <Button Content="Назад" Width="95" Height="35" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Left" Click="Button_Back" Background="#FF544DA5" FontWeight="Bold"/>
        <Button x:Name="nameStudBlock" Height="40" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Right" FontSize="22" Background="#FF544DA5"/>
    </Grid>
</Window>
