<Page x:Class="Sport_Application_Teacher__New_Design_.Pages.ReportStudHours"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sport_Application_Teacher__New_Design_.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="1080"
      Title="ReportStudHours">

    <Grid>
        <DataGrid x:Name="studHoursGrid" FontSize="22" AutoGeneratingColumn="studHoursGrid_AutoGeneratingColumn"/>
        <Button Content="Назад" Width="95" Height="35" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Left" Click="ButtonBack"/>
        <Button x:Name="nameStudBlock" Height="40" Margin="10" VerticalAlignment="Bottom" HorizontalAlignment="Right" FontSize="22"/>
    </Grid>
</Page>
